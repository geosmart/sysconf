{
  "beautify.options": {
    "preserve_newlines": true,
    "selector_separator_newline": false,
  },
  "editor.detectIndentation": false,
  "editor.fontFamily": "Courier Prime Code, OPPOSans",
  "editor.fontSize": 12,
  "editor.formatOnSave": false,
  "editor.letterSpacing": -1,
  "editor.lineHeight": 20,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true,
  },
  "editor.rulers": [
    80,
    120,
  ],
  "explorer.openEditors.visible": 0,
  "files.autoSave": "off",
  "files.exclude": {
    "**/*.retry": true,
    "**/__pycache__": true,
    "**/node_modules/": true,
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enabled": false,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "keyboard.dispatch": "keyCode",
  "python.formatting.provider": "yapf",
  "python.formatting.yapfPath": "/usr/bin/yapf",
  "python.linting.pylintArgs": [
    "--init-hook='import sys; sys.path.append(\"/usr/local/lib/python3.7/dist-packages\")'"
  ],
  "python.linting.pylintPath": "/usr/bin/pylint",
  "python.pythonPath": "/usr/bin/python3.7",
  "typescript.autoClosingTags": false,
  "vetur.format.defaultFormatter.ts": "vscode-typescript",
  "vim.cursorStylePerMode.normal": "line",
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": false,
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0.5,
  "workbench.colorTheme": "Atom Material Theme",
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "newUntitledFile",
  "[json]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[vue]": {
    "editor.formatOnSave": false,
  },
  "yaml.validate": false,
}
